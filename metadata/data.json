{
	"boundaries": {
		"name": "Atlas Admin Boundaries",
		"description": "Admin boundaries for the Atlas based on GAUL 2024. This includes admin 0, 1, and 2 levels.",
		"s3": {
			"bucket": "digital-atlas",
			"prefix_pattern": "domain=boundaries/type=admin/source=gaul2024/region={region}/processing=analysis-ready/level={level}/",
			"file_pattern": "atlas_gaul_a{level}_{region}.parquet"
		},
		"dimensions": {
			"region": [
				"global",
				"africa"
			],
			"level": [
				"0",
				"1",
				"2"
			]
		},
		"alternate-paths": {
			"project": "Data/boundaries/"
		}
	},
